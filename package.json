{
  "name": "ndefender-api-contracts",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "typecheck": "tsc -p tsconfig.json",
    "ci:links": "python3 ci/check_markdown_links.py",
    "ci:schemas": "python3 ci/check_schemas.py",
    "ci:openapi": "python3 ci/check_openapi.py",
    "ci:readme-openapi": "python3 ci/check_readme_openapi_sync.py",
    "ci:readme-examples": "python3 ci/readme_examples_lint.py",
    "ci:validate": "bash scripts/validate.sh",
    "ci": "npm run ci:links && npm run ci:schemas && npm run ci:openapi && npm run ci:readme-openapi && npm run ci:readme-examples && npm run typecheck && npm run ci:validate"
  },
  "devDependencies": {
    "typescript": "^5.4.5"
  }
}
